<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
            prefHeight="500" prefWidth="650"
            styleClass="bg-main"
            fx:controller="edu.uoc.uocycle.view.MainViewController">

    <StackPane fx:id="loginButtonContainer"
               styleClass="uoc-btn-primary-container"
               layoutX="208.0" layoutY="345.0">
        <Region styleClass="uoc-btn-container-hover" />
        <Button fx:id="loginButton"
                text="Start"
                onAction="#newLogin"
                styleClass="uoc-btn-primary"
                pickOnBounds="false"/>
    </StackPane>

    <StackPane fx:id="creditsButtonContainer"
               styleClass="uoc-btn-primary-container"
               layoutX="377.0" layoutY="345.0">
        <Region styleClass="uoc-btn-container-hover" />
        <Button fx:id="creditsButton"
                text="Credits"
                onAction="#readCredits"
                styleClass="uoc-btn-primary"
                pickOnBounds="false"/>
    </StackPane>
</AnchorPane>
